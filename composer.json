{
    "name": "lquintana/sendinblue",
    "description": "Laravel wraper to use sendinblue API",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Quintana",
            "email": "lquintana0707@gmail.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/lquintana/sendinblue",
    "keywords": ["Laravel", "Sendinblue", "Email Marketing"],
    "require": {
        "illuminate/support": "~9|~10",
        "sendinblue/api-v3-sdk": "^8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Lquintana\\Sendinblue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lquintana\\Sendinblue\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lquintana\\Sendinblue\\SendinblueServiceProvider"
            ],
            "aliases": {
                "Sendinblue": "Lquintana\\Sendinblue\\Facades\\Sendinblue"
            }
        }
    }
}
